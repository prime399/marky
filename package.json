{
  "name": "screenity",
  "version": "4.0.0",
  "description": "The free and privacy-friendly screen recorder with no sign in needed.",
  "main": "dist/main/main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build:electron",
    "build:extension": "cross-env NODE_ENV=production node utils/build.js",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.main.config.js",
    "build:preload": "cross-env NODE_ENV=production webpack --config webpack.preload.config.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.config.js",
    "build:electron": "npm run build:main && npm run build:preload && npm run build:renderer",
    "dev:renderer": "cross-env NODE_ENV=development webpack serve --config webpack.renderer.config.js",
    "dev:main": "cross-env NODE_ENV=development webpack --config webpack.main.config.js && cross-env NODE_ENV=development webpack --config webpack.preload.config.js",
    "dev:electron": "npm run dev:main && concurrently \"npm run dev:renderer\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "start": "cross-env NODE_ENV=development node utils/server.js",
    "dev": "npm run dev:electron . --disable-gpu --no-sandbox",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "hot-reload": "cross-env NODE_ENV=development webpack serve --hot",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "postinstall": "patch-package",
    "package": "npm run build && cd build && zip -r ../extension.zip . && cd ..",
    "package:electron": "npm run build:electron && electron-builder",
    "package:mac": "npm run build:electron && electron-builder --mac",
    "package:win": "npm run build:electron && electron-builder --win",
    "package:linux": "npm run build:electron && electron-builder --linux",
    "clean": "rm -rf build dist && mkdir build",
    "lint": "eslint src/**/*.js --fix",
    "test:unit": "node --test tests/phase0/projectSchema.test.js tests/infra/immerUpdate.test.js",
    "test:integration": "node --test tests/phase0/localProjectRepository.integration.test.js",
    "test:phase0": "npm run test:unit && npm run test:integration",
    "test:phase1:unit": "node --test tests/phase1/editorShellModel.unit.test.js",
    "test:phase1:integration": "node --test tests/phase1/editorShellSync.integration.test.js",
    "test:phase1": "npm run test:phase1:unit && npm run test:phase1:integration",
    "test:phase2:unit": "node --test tests/phase2/timelineModel.unit.test.js",
    "test:phase2:integration": "node --test tests/phase2/editorTimeline.integration.test.js",
    "test:phase2": "npm run test:phase2:unit && npm run test:phase2:integration",
    "test:runtime": "node --test tests/runtime/uiRuntimeContracts.test.js",
    "test:critical": "npm run test:phase0 && npm run test:phase1 && npm run test:phase2 && npm run test:runtime && npm run build"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@mediapipe/selfie_segmentation": "^0.1.1675465747",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-select": "^1.2.1",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.3",
    "@radix-ui/react-toast": "^1.2.13",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-toggle-group": "^1.1.9",
    "@radix-ui/react-toolbar": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@sentry/browser": "^7.94.1",
    "@tanstack/react-query": "^5.90.20",
    "@tensorflow-models/body-segmentation": "^1.0.2",
    "@tensorflow/tfjs-backend-webgl": "^4.9.0",
    "@tensorflow/tfjs-converter": "^4.9.0",
    "@tensorflow/tfjs-core": "^4.13.0",
    "@uiw/color-convert": "^1.2.2",
    "@uiw/react-color-wheel": "^1.2.2",
    "axios": "^1.6.2",
    "dotenv": "^17.0.1",
    "electron-store": "^8.2.0",
    "fabric": "^5.3.0",
    "fix-webm-duration": "^1.0.5",
    "gif.js": "^0.2.0",
    "immer": "^11.1.3",
    "jszip": "^3.10.1",
    "localforage": "^1.10.0",
    "mediabunny": "^1.24.4",
    "patch-package": "^8.0.0",
    "plyr": "3.7.8",
    "plyr-react": "^5.3.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-advanced-cropper": "^0.19.4",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-rnd": "^10.4.1",
    "react-shadow": "^20.0.0",
    "react-svg": "^16.1.18",
    "ssestream": "^1.1.0",
    "wavesurfer.js": "^7.4.2",
    "webext-bridge": "^6.0.1",
    "webm-duration-fix": "^1.0.4",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.27.1",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "fs-extra": "^10.1.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.2.0",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.1",
    "typescript": "^5.9.3",
    "wait-on": "^8.0.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.3",
    "webpack-merge": "^6.0.1"
  },
  "overrides": {
    "tar": "7.5.7"
  }
}
